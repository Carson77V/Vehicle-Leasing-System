+DieselTruck(dTruck:DieselTruck)
+getFuelCapacity():int
+setFuelCapacity(fuelCapacity:int):void
+equals(obj:Object):boolean
+toString():String
}

class Car{
#maxPassengers:int
+Car()
+Car(maxPassengers:int, make:String, model:String, yearProduced:int)
+Car(car:Car)
+getMaxPassengers():int
+setMaxPassengers(max:int):void
+equals(obj:Object):boolean
+toString():String
}

Car <|-- ElectricCar
Car <|-- GasCar

class ElectricCar{
#range:int
+ELECTRICCARPLATE:String
#eCarPlateNum:int
+ElectricCar()
