@startuml
package vehicles{
class Vehicle{
#plateNumber:int
#make:String
#model:String
#yearProduced:int

+Vehicle()
+Vehicle(make:String, model:String, yearProduced:int)
+Vehicle(vehicle:Vehicle)
+setPlateNumber(num:int):void
+setMake(make:String):void
+setModel(model:String):void
+setYearProduced(yearProduced:int):void
+getPlateNumber():int
+getMake():String
+getModel():String
+getYearProduced():int
+toString():String
+equals(obj:Object):boolean
+copyVehicles(obj:Object):Vehicle[]
}

Vehicle <|-- Truck
Vehicle <|-- Car

class Truck{
#capacity:int
+Truck()
+Truck(capacity:int, make:String, model:String, yearProduced:int)
+Truck(truck:Truck)
+getCapacity():int
+setCapacity(capacity:int):void
+toString():String
+equals(obj:Object):boolean
+getLargestTruck(truck:Truck[]):Truck
}

Truck <|-- ElectricTruck
Truck <|-- DieselTruck

class ElectricTruck{
#range:int
+ELECTRICTRUCKPLATE:String
#eTruckPlateNum:int
+ElectricTruck()
+ElectricTruck(range:int, capacity:int, make:String, model:String, yearProduced:int)
+ElectricTruck(eTruck:ElectricTruck)
+getRange():int
+setRange(range:int):void
+equals(obj:Object):boolean
+toString():String
}

class DieselTruck{
-fuelCapacity:int
+DIESELTRUCKPLATE:String
#dTruckPlateNum:int
+DieselTruck()
